public class Main {
    public static void main(String[] args) {
        int[] array = new int[]{16, 17, 18, 19, 21,22};
        for (int i = 0; i < array.length; i++) {
            array[i] = array[i] / 5;
            System.out.println(array[i]);

        }
         System.out.println("16, 17, 18, 19, 21,22");
         
    }
}
