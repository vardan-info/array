public class Main {
    public static void main(String[] args) {
        int[] array = new int[]{10, 11, 12, 13, 14,15};
        for (int i = 0; i < array.length; i++) {
            array[i] = array[i] / 3;
            System.out.println(array[i]);

        }
         System.out.println("11 12 13 14");
    }
}
