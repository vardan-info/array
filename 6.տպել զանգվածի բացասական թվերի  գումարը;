public class Main {
    public static void main(String[] args) {

        int[] array3 = new int[4];

        array3[0]=-5;
        array3[1]=-17;
        array3[2]=-350;
        array3[3]=450;

        int sum = 0;

        for( int i = 0; i < array3.length; i++){
            sum += array3[i];
        }

        for( int i = 0; i < array3.length; i++){
            System.out.println(array3[i]);
        }

        System.out.println("tvern - "+ sum);

    }
}
