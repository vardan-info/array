public class Main {
    public static void main(String[] args) {
        int[] array = new int[5];
        array[0] = 50;
        array[1] = 15;
        array[2] = 20;
        array[3] = 25;
        array[4] = 30;
        int sum = 0;
        for (int i = 0; i < array.length; i++) {
            sum += array[i];
        }
        System.out.println(sum);
        System.out.println("15,20,25,30");
    }
}
