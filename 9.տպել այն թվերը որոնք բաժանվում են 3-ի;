public class Main {
    public static void main(String[] args) {
        int[] array = new int[]{1, 2, 3, 4, 5,6};
        for (int i = 0; i < array.length; i++) {
            array[i] = array[i] / 3;
            System.out.println(array[i]);

        }
         System.out.println("2  4 5");
    }
}
